<%- include("partials/header"); %>
<div class="content-wrap container-fluid">
<h1 class="heading pt-4">Subjects For <%=deptCode.toUpperCase()%> Semester <%=semester%></h1>
<div class="row pt-3 pb-3 mx-lg-4 px-3 px-lg-5">
<% subjects.forEach((subject)=> { %>
<div class="col-lg-4 pb-3">
<div class="card subject-card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title mb-3"><a href="/subjects/<%= subject.code %>"><%= subject.name %></a></h5>
    <h5 class="card-subtitle mb-2 text-body-secondary"><%= subject.code %></h6>
  </div>
  </div>
</div>
<% }); %>
</div>
</div>
<%- include("partials/footer"); %>